{
  "name": "kitchen-helper",
  "version": "0.0.1",
  "description": "Smart Kitchen Helper",
  "main": "src/app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "db:migrate": "npx sequelize-cli migration:generate --name migration-skeleton",
    "db:seed": "npx sequelize-cli seed:generate --name seeder-skeleton",
    "start:db:migration": "npx sequelize-cli db:migrate --env development",
    "undo:db:migration": "npx sequelize-cli db:migrate:undo",
    "start:db:seeder": "npx sequelize-cli db:seed:all --env development",
    "undo:db:seeder": "npx sequelize-cli db:seed:undo:all",
    "seeder": "nodemon ./src/common/loaders/seeder.ts"
  },
  "author": "Sagar Parmar",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "googleapis": "^134.0.0",
    "helmet": "^7.0.0",
    "jm-ez-l10n": "^1.0.0",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "natural": "^6.10.3",
    "nodemailer": "^6.7.5",
    "npm-check-updates": "^17.0.2",
    "pg": "^8.12.0",
    "redis": "^4.6.10",
    "sequelize": "^6.37.5",
    "typedi": "^0.10.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.16.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.8.0",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "18.18.2",
    "npm": "9.8.1"
  }
}
